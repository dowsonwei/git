<html>
<head>
	<meta http-equiv="content-type" content="text/html;charset=utf-8">
	<title></title>
	<link rel="stylesheet" type="text/css" href="css/base.css">
	<script type="text/javascript" src="js/jquery-2.1.1.min.js"></script>
	<style type="text/css">
	.btn {
		text-indent: 0;
	}
	table#hidden-line{
		margin: 30px auto;
	}
	div#content{
		text-align: center;
	}
	</style>
	<script type="text/javascript">
		function changeChart(o){
			var type = o.value;
			if(type == 1){
				$("#chart").attr("src","img/zz.png")
			}else if(type == 2){
				$("#chart").attr("src","img/zx.png")
			}else if(type == 3){
				$("#chart").attr("src","img/bz.png")
			}
		}
	</script>
</head>
<body>
	<table id="hidden-line">
		<tr>
			<td style="width: 200px;">选择查看图表类型</td>
			<td style="width: 150px;">
				<select id="chartType" onchange="changeChart(this);">
					<option value="1">柱状图</option>
					<option value="2">折线图</option>
					<option value="3">饼状图</option>
				</select>
			</td>
		</tr>
	</table>
	<div id="content">
		<img id="chart" alt="图表" src="img/zz.png">
	</div>
	<div style="text-align: center;margin-top: 50px;">
		<a href="javascript:window.history.go(-1);" class="btn" >返回</a>
	</div>
</body>
</html>