	<script type="text/javascript">
		function changeList(node,num){
			var collANodes = document.getElementsByName("alist");
			
			var div_id = "list_" + num;
			var oNode = document.getElementById(div_id);
			//设置a和div的样式
			oNode.className = "content openDiv";
			node.className = "aover";
			for(var i=0;i<collANodes.length;i++){
				if(i != num){
					//除了当前鼠标悬停的a和对应的div外，其他都设置为原始状态
					document.getElementById("list_"+i).className = "content closeDiv";
					collANodes[i].className = "aout";
				}
			}
		}
	</script>
